<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.mengtac.templatehistorischekilometer.Historische_kilometer">

    <TextView
        android:id="@+id/activityhistorischekilometer_textView_historischekilometer_id"
        android:layout_width="406dp"
        android:layout_height="52dp"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="0dp"
        android:text="@string/titel"
        android:textSize="42sp"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:text="Historische kilometer" />

    <TextView
        android:id="@+id/activityhistorischekilometer_textView_looptijd_id"
        android:layout_width="220dp"
        android:layout_height="52dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="100dp"
        android:text="@string/looptijd"
        android:textSize="24sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/activityhistorischekilometer_textView_historischekilometer_id"
        tools:ignore="RtlHardcoded"
        tools:text="Geschatte looptijd:" />

    <TextView
        android:id="@+id/activityhistorischekilometer_textView_afstand_id"
        android:layout_width="243dp"
        android:layout_height="52dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:text="@string/afstand"
        android:textSize="24sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/activityhistorischekilometer_textView_looptijd_id"
        tools:ignore="RtlHardcoded"
        tools:text="Afstand (kilometer):" />

    <TextView
        android:id="@+id/activityhistorischekilometer_textView_looptijdvalue_id"
        android:layout_width="78dp"
        android:layout_height="52dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="16dp"
        android:textSize="24sp"
        app:layout_constraintHorizontal_bias="0.506"
        app:layout_constraintLeft_toRightOf="@+id/activityhistorischekilometer_textView_looptijd_id"
        app:layout_constraintRight_toRightOf="parent"
        tools:text="45.00"
        android:layout_marginTop="100dp"
        app:layout_constraintTop_toBottomOf="@+id/activityhistorischekilometer_textView_historischekilometer_id"
        tools:ignore="RtlHardcoded" />

    <TextView
        android:id="@+id/activityhistorischekilometer_textView_afstandvalue_id"
        android:layout_width="79dp"
        android:layout_height="52dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="8dp"
        android:textSize="24sp"
        app:layout_constraintHorizontal_bias="0.315"
        app:layout_constraintLeft_toRightOf="@+id/activityhistorischekilometer_textView_afstand_id"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/activityhistorischekilometer_textView_looptijdvalue_id"
        tools:text="10"
        tools:ignore="RtlHardcoded" />

    <Button
        android:id="@+id/activityhistorischekilometer_button_startroute_id"
        android:layout_width="406dp"
        android:layout_height="52dp"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginTop="0dp"
        android:text="@string/startroute"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/activityhistorischekilometer_textView_afstand_id"
        app:layout_constraintVertical_bias="1.0"
        tools:text="Start route" />
</android.support.constraint.ConstraintLayout>
